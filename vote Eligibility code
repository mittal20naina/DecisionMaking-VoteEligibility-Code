#include<iostream>
using namespace std;
int main()
{
	int age;
	cout<<"enter the age of person:";
	cin>>age;
	if(age<13)
	{
		cout<<"childhood age!!\n";
	}
	else
	if(age>=13 && age<=20)
	{
		cout<<"teenager age!!\n";
	}
	else
	if(age>20 && age<=60)
	{
		cout<<"adulthood age!!\n";
	}
	else
	{
		cout<<"old age!!\n";
	}
	if(age>=18)
	{
		cout<<"the person is eligible for vote!\n";
	}
	else
	{
		cout<<"the person is not eligible for vote!\n";
	}
	return 0;
}
